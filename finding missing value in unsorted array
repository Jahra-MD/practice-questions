// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int arr[]={2,3,1,5,5};
    int n =sizeof(arr)/sizeof(arr[0]);
    int ans[n]={0};
    for(int i=0;i<n;i++)
    {
        ans[arr[i]-1] =1;
    }
    for(int i=0;i<n;i++)
    {
        if(ans[i]==0)
        {
            cout<<i+1;
        }
    }

    return 0;
}


output
4
