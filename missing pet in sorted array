// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int MissingPet(int arr[],int n)
{   int res=0;
    for(int i=1;i<n;i++)
    {
       if(i!=arr[i])
       {
          res =i;
       }
    }
    return res;
}
int main() {
   int arr[]={1,2,3,4,6,7};
   int n =sizeof(arr)/sizeof(arr[0]);
   cout<<"the missing pet is \n"<<MissingPet(arr,n);
    return 0;
}


o/p
the missing pet is 
5
