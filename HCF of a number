// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int a,b;
    cout<<"enter the two numbers: \n";
    cin>>a>>b;
    if(a>b)
    {
        int temp =a;
        a =b;
        b =temp;
    }
   for(int i =a;i>0;i--)
   {
       if( a%i==0 && b%i==0)
       {
            cout<<i;
            break;
       }
   }
}



