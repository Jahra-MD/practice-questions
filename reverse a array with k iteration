// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
   int arr[5]={1,2,3,4,5};
   int n =sizeof(arr)/sizeof(arr[0]);
   int k;
   cout<<"enter the reverse count \n"<<endl;
   cin>>k;
   for(int i=0;i<n;i++)
   {
       cout<<arr[(k-i-1)%k]<<" ";
   } 
}

