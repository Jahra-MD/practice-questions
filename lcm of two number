// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    int a,b;
    cout<<"enter the two numbers: \n";
    cin>>a>>b;
    int max =1;
    while(1)
    {
        if(max%a ==0 && max%b ==0)
        {
            cout<<"the lcm of given number is \n"<<max;
            break;
        }
        ++max;
    }
    return 0;
}

o/p
enter the two numbers: 
60 42
the lcm of given number is 
420
